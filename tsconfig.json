{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "baseUrl": "./src",
    "outDir": "build",
    "paths": {
      "@component/*": ["component/*"],
      "@style/*": ["style/*"]
    }
  },
  "include": ["express-server.ts", "src"]
}
